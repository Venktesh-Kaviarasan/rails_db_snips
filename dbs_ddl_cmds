select *
from dbs_collateral_master;

EXPLAIN
SELECT *
FROM dbs_collateral_master
WHERE type_description = 'Immovable collateral type';

CREATE INDEX idx_type_description
    ON dbs_collateral_master (type_description);

create table dbs_industry_master
(
    industry_name text,
    industry_code varchar(100),
    code_type_cd  varchar(100)
);

select *
from dbs_industry_master;

