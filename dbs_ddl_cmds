create table dbs_collateral_master
(
    code_description      text,
    type_code             varchar(100),
    type_description      text,
    class_code            varchar(100),
    class_description     varchar(255),
    collateral_code       varchar(100),
    src_collateral_code_1 varchar(10),
    src_collateral_code_2 varchar(10)
);

